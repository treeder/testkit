{
  "name": "testkit",
  "compatibility_date": "2025-08-11",
  "main": "./dist/index.js",
  "assets": {
    "directory": "./public/",
    "binding": "ASSETS",
  },
  "build": {
    "command": "npx treeder/flaregun build",
    "cwd": ".",
    "watch_dir": ["functions", "public"],
  },
  "env": {
    "dev": {
      "name": "testkit-dev",
      "vars": {
        "ENV": "dev",
      },
    },
  },
}